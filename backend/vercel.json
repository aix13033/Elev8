
{
  "buildCommand": "./node_modules/.bin/expo export -p web",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/"
    }
  ],
  "crons": [
    { "path": "/api/cron/nightly-oura?key=${CRON_SECRET}", "schedule": "0 4 * * *" },
    { "path": "/api/cron/daily-agent?key=${CRON_SECRET}", "schedule": "0 7 * * *" },
    { "path": "/api/cron/weekly-plans?key=${CRON_SECRET}", "schedule": "0 6 * * 1" }
  ]
}